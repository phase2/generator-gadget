  # Provides cache drivers for <%= cache %>. Module installation not required.
  <%= cache %>:
    version: "<%= cacheVersion %>"
<% if (cache == 'redis') { -%>
libraries:

  ##
  # Predis
  # - https://github.com/nrk/predis is a userland Redis client for PHP
  # - PhpRedis (https://github.com/phpredis/phpredis) is a PHP extension with
  #   slightly higher performance and should be used if possible.
  ##
  predis:
    download:
      type: "get"
      url: "https://github.com/nrk/predis/archive/v1.0.3.zip"
    directory_name: "predis"
    type: "library"
<% } -%>
